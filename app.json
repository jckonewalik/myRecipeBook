{
  "expo": {
    "name": "My Recipe Book",
    "slug": "myRecipeBook",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jcstecnologia.myrecipebook",
      "buildNumber": "1.4.0"
    },
    "android": {
      "package": "com.jcstecnologia.myrecipebook",
      "versionCode": 5,
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "60ad0b97-a5c0-4da7-8613-3145f0209cd7"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": false,
          "microphonePermission": false
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-sqlite"
    ]
  }
}
